{
  "name": "ob.js",
  "version": "0.0.1",
  "description": "A typescript library for managing javascript objects",
  "scripts": {
    "build": "tsc",
    "build-single" : "cp -f tsconfig.json src && tsc --declaration --module amd --project src --outFile build/ob.js && rm -f src/tsconfig.json && npm run uglify-build-single",
    "watch": "tsc -w",
    "uglify-build-single": "uglifyjs --compress --mangle --source-map build/ob.min.map --output  build/ob.min.js -- build/ob.js",
    "test": "npm run build && karma start",
    "test-once": "npm run build && karma start --single-run",
    "karma": "karma",
	"uglifyjs": "uglifyjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaenyph/objs.git"
  },
  "keywords": [
    "objects",
    "javascript",
    "library",
    "typescript"
  ],
  "author": "jaenyph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaenyph/objs/issues"
  },
  "homepage": "https://github.com/jaenyph/objs#readme",
  "devDependencies": {
    "http-server": "~0.9.0",
    "jasmine": "^2.5.3",
    "jasmine-core" : "~2.5.2",
    "karma": "^1.4.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-requirejs": "^1.1.0",
    "requirejs": "~2.3.2",
    "tslint": "~3.15.0",
    "typescript": "~2.1.0",
    "uglify-js": "~2.7.5"
  }
}
